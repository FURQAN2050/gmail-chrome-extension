{
  "name": "group",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "emails": {
      "type": "hasMany",
      "model": "email",
      "foreignKey": "groupId",
      "options": {
        "nestRemoting": true
      },
      "through": "groupemail",
      "keyThrough": "emailId"
    },
    "templates": {
      "type": "hasMany",
      "model": "template",
      "foreignKey": "groupId",
      "options": {
        "nestRemoting": true
      },
      "through": "grouptemplate",
      "keyThrough": "templateId"
    }
  },
  "acls": [],
  "methods": {}
}
